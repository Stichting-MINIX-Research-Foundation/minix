/*
 * iso-8859-8, known as Latin-Hebrew (ESC , H)
 *
 * XXX is it correct? i'm not sure.
 */
CHARSET		",H"

ALPHA		0x60 - 0x7a
GRAPH		0x22 - 0x3e 0x5f 0x60 - 0x7a
PUNCT		0x22 - 0x3e
SPACE		0x09 - 0x0d 0x20
BLANK		0x09 0x20
PRINT		0x20 0x22 - 0x3e 0x5f 0x60 - 0x7a
SWIDTH1		0x20 - 0x7f
