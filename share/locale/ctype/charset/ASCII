/*	$NetBSD: ASCII,v 1.1 2000/12/30 02:33:20 itojun Exp $	*/

/*
 * ASCII Code Set (ESC ( B)
 */
CHARSET		"(B"

ALPHA		'A' - 'Z' 'a' - 'z'
CONTROL		0x00 - 0x1f 0x7f
DIGIT		'0' - '9'
GRAPH		0x21 - 0x7e
LOWER		'a' - 'z'
PUNCT		0x21 - 0x2f 0x3a - 0x40 0x5b - 0x60 0x7b - 0x7e
SPACE		0x09 - 0x0d 0x20
UPPER		'A' - 'Z'
XDIGIT		'a' - 'f' 'A' - 'F'
BLANK		' ' '\t'
PRINT		0x20 - 0x7e
SWIDTH1         0x20 - 0x7e

MAPLOWER       	< 'A' - 'Z' : 'a' >
MAPLOWER       	< 'a' - 'z' : 'a' >
MAPUPPER       	< 'A' - 'Z' : 'A' >
MAPUPPER       	< 'a' - 'z' : 'A' >
TODIGIT       	< '0' - '9' : 0 >
TODIGIT       	< 'A' - 'F' : 10 >
TODIGIT       	< 'a' - 'f' : 10 >
