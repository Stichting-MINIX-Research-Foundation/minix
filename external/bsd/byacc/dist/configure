	rm -f $cf_name.html $cf_name.pdf $cf_name.ps $cf_name.txt

$cf_name.html : $cf_name.\1
$cf_name.pdf : $cf_name.ps
$cf_name.ps : $cf_name.\1
$cf_name.txt : $cf_name.\1
CF_EOF
