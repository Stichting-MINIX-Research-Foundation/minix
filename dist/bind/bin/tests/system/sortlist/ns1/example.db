$TTL 300	; 5 minutes
@			IN SOA	ns1.example. hostmaster.example. (
				2000042795 ; serial
				20         ; refresh (20 seconds)
				20         ; retry (20 seconds)
				1814400    ; expire (3 weeks)
				3600       ; minimum (1 hour)
				)
example.		NS	ns1.example.
ns1.example.		A	10.53.0.1

