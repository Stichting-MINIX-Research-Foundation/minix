$TTL 300	; 5 minutes
@			IN SOA	mname1. . (
				2000042407 ; serial
				20         ; refresh (20 seconds)
				20         ; retry (20 seconds)
				1814400    ; expire (3 weeks)
				3600       ; minimum (1 hour)
				)
			NS	ns
ns			A	10.53.0.3

a			A	10.0.0.1
b			A	10.0.0.2
d			A	10.0.0.4
z			A	10.0.0.26
a.a.a.a.a.a.a.a.a.a.e	A	10.0.0.27
x			CNAME	a

private			NS	ns.private
ns.private		A	10.53.0.2

insecure		NS	ns.insecure
ns.insecure		A	10.53.0.2

