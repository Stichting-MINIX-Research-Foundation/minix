    int main() {
	int i;
	/* list of codeset names likely to exist */
	static char *codeset[] = {
	    "ASCII", "ISO-8859-1", "ISO8859-1", "iso8859_1", "8859-1",
	    "Shift_JIS", "SJIS", "sjis", NULL,
