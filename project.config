[project]
	description = Minix repository
[submit]
	action = fast forward only
[access "refs/heads/*"]
	create = group Minix admins
	forgeAuthor = group Administrators
	forgeAuthor = group Minix devs
	forgeCommitter = group Administrators
	forgeCommitter = group Minix devs
	label-Verified = -1..+1 group Non-Interactive Users
	submit = group Minix devs
	push = +force group Administrators
	pushMerge = group Minix admins
	pushTag = group Administrators
[access "refs/tags/*"]
	create = group Administrators
	forgeAuthor = group Administrators
	forgeCommitter = group Administrators
